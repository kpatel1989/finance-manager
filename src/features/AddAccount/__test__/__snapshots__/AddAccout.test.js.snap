exports[`renders the Add Account component 1`] = `
<div
  className="card"
>
  <div
    className="card-body"
  >
    <div
      className="card-title h5"
    >
      Add a new account
    </div>
    <form
      align="left"
      className=""
    >
      <div
        className="form-group row"
      >
        <label
          className="form-label col-form-label col-sm-4"
          htmlFor="accountName"
        >
          Account Name
        </label>
        <div
          className="col-sm-8"
        >
          <input
            className="form-control"
            id="accountName"
            onChange={[Function]}
            placeholder="Enter account name"
            type="text"
            value=""
          />
        </div>
      </div>
      <div
        className="form-group row"
      >
        <label
          className="form-label col-form-label col-sm-4"
          htmlFor="accountType"
        >
          Account Type
        </label>
        <div
          className="col-sm-8"
        >
          <select
            className="custom-select"
            data-testid="bankTypeSelector"
            id="accountType"
            onChange={[Function]}
            value=""
          >
            <option>
              Select the type of account
            </option>
            <option
              data-testid="bankType-{ACCOUNT_TYPES.chequing}"
              value="Chequing"
            >
              Chequing
            </option>
            <option
              data-testid="bankType-{ACCOUNT_TYPES.savings}"
              value="Savings"
            >
              Savings
            </option>
            <option
              data-testid="bankType-{ACCOUNT_TYPES.creditCard}"
              value="Credit Card"
            >
              Credit Card
            </option>
            <option
              data-testid="bankType-{ACCOUNT_TYPES.rrsp}"
              value="RRSP"
            >
              RRSP
            </option>
            <option
              data-testid="bankType-{ACCOUNT_TYPES.tfsa}"
              value="TFSA"
            >
              TFSA
            </option>
          </select>
        </div>
      </div>
      
      <button
        className="btn btn-primary"
        disabled={false}
        onClick={[Function]}
        type="button"
      >
        Save
      </button>
    </form>
  </div>
</div>
